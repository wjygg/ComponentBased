<?xml version="1.0" encoding="utf-8"?>
<resources>
<declare-styleable name="QQView">
    <!--左圆弧的颜色-->
    <attr name="QQViewInnerColor" format="color"></attr>
    <!--右圆弧的颜色-->
    <attr name="QQViewOutnerColor" format="color"></attr>
    <!--圆弧的宽度-->
    <attr name="QQViewBorderWidth" format="dimension"></attr>
    <!--字体的颜色-->
    <attr name="QQViewTextColor" format="color"></attr>
    <!--字体的大小-->
    <attr name="QQViewTextSize" format="dimension"></attr>
    <!--字体-->
    <attr name="QQViewText" format="string"></attr>
</declare-styleable>

<declare-styleable name="TestTextView">

    <attr name="TestTextViewText" format="string"></attr>
    <attr name="TestTextViewTextSize" format="dimension"></attr>
    <attr name="TestTextViewTextColor" format="color"></attr>
</declare-styleable>




<declare-styleable name="SlidingDiscolorationTextView">


    <attr name="SlidingChangeColor" format="color"></attr>
    <attr name="SlidingColor" format="color"></attr>
    <attr name="SlidingTextSize" format="dimension"></attr>
    <attr name="SlidingText" format="string"></attr>
</declare-styleable>


    <declare-styleable name="SlidingLettersView">
        
        <attr name="SlidingLettersTextSize" format="dimension"></attr>
        <attr name="SlidingLettersTextInnerColor" format="color"></attr>
        <attr name="SlidingLettersTextOutnerColor" format="color"></attr>
    </declare-styleable>

    <!--自定义圆形比例控件-->
    <declare-styleable name="CircularScaleDiagramView">
        <!--中间字体-->
        <attr name="CircularScaleDiagramViewText" format="string"></attr>
        <!--字体大小-->
        <attr name="CircularScaleDiagramViewTextSize" format="dimension"></attr>
        <!--字体颜色-->
        <attr name="CircularScaleDiagramViewTextColor" format="color"></attr>
        <!--内部圆颜色-->
        <attr name="CircularScaleDiagramViewCircleColor" format="color"></attr>
        <!--圆弧颜色-->
        <attr name="CircularScaleDiagramViewArcColor" format="color"></attr>
        <!--圆弧宽度-->
        <attr name="CircularScaleDiagramViewArcWidth" format="dimension"></attr>
    </declare-styleable>

    <declare-styleable name="BannerView">
        <!--宽度比例-->
        <attr name="BannerViewWidth" format="float"></attr>
        <!--高度比例-->
        <attr name="BannerViewHeight" format="float"></attr>

    </declare-styleable>
</resources>
